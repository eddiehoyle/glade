
/* Section
 */
QLabel#sectionHeaderLabel {
    margin-left: 5px;
    font-weight: bold;
}
QLabel#sectionHeaderCount {
    background-color: "#444444";
    padding: 2px;
    border-radius: 4px;
    font-size: 9pt;
}
QFrame#sectionHeader {
    background-color: "#555555";
    margin: 0px;
    padding: 4px;
}
QFrame#sectionHeader:hover {
    background-color: "#5A5A5A";
}
QFrame#sectionBody {
    margin: 0px;
    padding: 0px;
}
QPushButton#load {
    padding: 0px;
    margin: 0px;
}

/* Plugin
 */
 QLabel#itemPluginName {
    margin-left: 2px;
}
QFrame#itemHeader {
    background-color: "#333333";
    margin: 0px;
    padding: 4px;
    font-size: 11pt;
}
QFrame#itemHeader:hover {
    background-color: "#3A3A3A";
}
QFrame#itemBody {
    background-color: "#222222";
    margin: 0px;
    padding: 5px;
}
QLabel#itemDataKey {
    margin: 4px;
    padding: 0px;
    font-size: 11pt;
}
QLineEdit#itemDataValue {
    margin: 0px;
    padding: 2px;
    border: none;
    font-size: 11pt;
}

/* Window
 */
QLineEdit#searchField {
    padding: 5px;
    margin: 0px;
}
QFrame#pluginList {
    background-color: "#333333";
    border: 1px solid "#333333";
}